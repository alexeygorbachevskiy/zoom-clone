{
  "name": "zoom-clone-backend",
  "version": "0.1.0",
  "description": "Zoom clone backend",
  "scripts": {
    "build": "npm run build-ts",
    "start": "nodemon src/server.ts",
    "start-build": "npm run serve",
    "vercel-build": "echo hello",
    "build:watch": "tsc --watch",
    "serve": "node build/_server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-node": "nodemon build/_server.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "eslint src/*.ts",
    "lint-fix": "yarn lint --fix",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect build/_server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "typecheck": "tsc --noEmit",
    "run-prettier": "prettier src/*.ts",
    "validate-prettiness": "yarn run-prettier -c",
    "make-prettier": "yarn run-prettier --write"
  },
  "engines": {
    "node": "18"
  },
  "main": "build/index.js",
  "files": [
    "build",
    "index.js"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.16.2",
    "http-errors": "^1.7.1",
    "morgan": "^1.9.1",
    "pusher": "^5.1.3",
    "redis": "^4.6.7",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "4.17.17",
    "@types/http-errors": "1.8.2",
    "@types/morgan": "1.9.4",
    "@types/node": "14.18.53",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "^6.1.0",
    "concurrently": "6.5.1",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "nodemon": "2.0.22",
    "prettier": "2.8.8",
    "typescript": "4.9.5"
  }
}
